{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@ostore/utils": ["packages/utils/index.ts"],
      "@ostore/ui": ["packages/components/index.ts"],
      "@ostore/*": ["packages/components/*"],
    },
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "skipLibCheck": true,
    "stripInternal": true,
    "resolvePackageJsonExports": true
  },
  "include": [".dumirc.ts", "**/*"],
  "exclude": ["node_modules", "lib", "es"]
}
